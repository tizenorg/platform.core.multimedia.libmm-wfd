<manifest>
	<define>
		<domain name="miracast-server"/>
		<permit>
			<smack permit="system::use_internet" type="w"/>
		</permit>
		<request>
			<smack request="system::use_internet" type="w"/>
		</request>
	</define>
    	<request>
        	<domain name="_"/>
    	</request>
    	<assign>
        	<filesystem path="/usr/bin/miracast_server" label="miracast-server" exec_label="miracast-server"/>
    	</assign>
</manifest>
